{
  "name": "kuvo-proxy",
  "productName": "kuvo-proxy",
  "version": "1.0.0",
  "description": "A Man-In-The-Middle Attack that blocks access to Kuvo and instead sends the currently playing track to a custom location or write it to file.",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": {
    "name": "Tobias S. Keller",
    "email": "tobias.s.keller@gmail.com"
  },
  "license": "GNU AGPLv3",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "my_new_app"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.54",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/anyproxy": "^4.1.1",
    "@types/express": "^4.17.11",
    "@types/http-proxy": "^1.17.5",
    "@types/pem": "^1.9.5",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/rimraf": "^3.0.0",
    "@types/ws": "^7.4.1",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "css-loader": "^4.3.0",
    "electron": "12.0.2",
    "electron-reloader": "^1.2.0",
    "eslint": "^7.23.0",
    "eslint-plugin-import": "^2.22.1",
    "fork-ts-checker-webpack-plugin": "^5.2.1",
    "node-loader": "^1.0.2",
    "style-loader": "^1.3.0",
    "ts-loader": "^8.1.0",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "clsx": "^1.1.1",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.17.1",
    "http-proxy": "^1.18.1",
    "json-colorizer": "^2.2.2",
    "nanoid": "^3.1.22",
    "pem": "^1.14.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "sudo-prompt": "^9.2.1"
  }
}
